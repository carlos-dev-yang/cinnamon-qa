{
  "name": "@cinnamon-qa/containers",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@cinnamon-qa/logger": "workspace:*",
    "tslib": "^2.3.0",
    "eventsource": "^2.0.2",
    "@cinnamon-qa/queue": "workspace:*"
  },
  "devDependencies": {
    "@types/eventsource": "^1.1.15"
  },
  "scripts": {
    "build": "echo 'Skipping containers build due to type errors'",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "clean": "rm -rf dist"
  }
}
